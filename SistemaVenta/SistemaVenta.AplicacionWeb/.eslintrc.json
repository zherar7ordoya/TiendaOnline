{
    "env":
    {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions":
    {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules":
    {
        "no-undef": "error",
        "no-unused-vars": "warn",
        "no-console": "off"
    }
}